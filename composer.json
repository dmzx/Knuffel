{
	"name": "dmzx/knuffel",
	"type": "phpbb-extension",
	"description": "With this extension you can add a Knuffel game to your forum Knuffel (Kniffel-Clone, also known as Yahtzee or dicepoker) is a dice game, where it's your aim to roll certain figures and reach a maximum score out of them.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.4",
	"time": "2021-03-04",
	"keywords": [
		"phpbb",
		"extension",
		"knuffel"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"composer/installers": "~1.0.0"
	},
	"extra": {
		"display-name": "Knuffel",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<4.0.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "knuffel_version.json",
			"ssl": true
		}
	}
}
